{
  "name": "routerino",
  "version": "2.2.0",
  "description": "A lightweight, SEO-optimized React router for modern web applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerds-with-keyboards/routerino.git"
  },
  "keywords": [
    "router",
    "react",
    "seo",
    "prerender",
    "react router"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nerds-with-keyboards/routerino/issues"
  },
  "homepage": "https://github.com/nerds-with-keyboards/routerino#readme",
  "files": [
    "dist",
    "types",
    "routerino-forge.js",
    "prerender",
    "docs"
  ],
  "type": "module",
  "types": "types/routerino.d.ts",
  "main": "./dist/routerino.umd.cjs",
  "exports": {
    ".": {
      "types": "./types/routerino.d.ts",
      "import": "./dist/routerino.js",
      "require": "./dist/routerino.umd.cjs"
    },
    "./forge": {
      "import": "./routerino-forge.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:app": "cd test-apps/vite-app && npm run build",
    "test:integration": "npm run build && npm run test:app",
    "test:react-versions": "node test-react-versions.js",
    "test:node-versions": "node test-node-versions.js",
    "prepublishOnly": "vite build",
    "lint": "eslint --fix . --max-warnings=0",
    "lint:fix": "eslint --fix .",
    "fix:entities": "find demo-prerender demo-static -name '*.jsx' -exec sed -i '' 's/\"/{\\\"}/g; s/'\"'\"'/{\\\"'\"'\"'}/g' {} +",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "express": "^4.18.2",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "22.16.0",
    "npm": "10.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,cjs}": [
      "eslint --fix --max-warnings=0 --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,html}": [
      "prettier --write"
    ]
  }
}
