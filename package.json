{
  "name": "routerino",
  "version": "1.1.9",
  "description": "A lightweight, SEO-optimized React router for modern web applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerds-with-keyboards/routerino.git"
  },
  "keywords": [
    "router",
    "react",
    "seo",
    "prerender",
    "react router"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nerds-with-keyboards/routerino/issues"
  },
  "homepage": "https://github.com/nerds-with-keyboards/routerino#readme",
  "files": [
    "dist",
    "types"
  ],
  "bin": {
    "routerino-build-sitemap": "build-sitemap.js",
    "build-sitemap": "build-sitemap.js"
  },
  "type": "module",
  "types": "types/routerino.d.ts",
  "main": "./dist/routerino.umd.cjs",
  "exports": {
    "types": "./types/routerino.d.ts",
    "import": "./dist/routerino.js",
    "require": "./dist/routerino.umd.cjs"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:react-versions": "node test-react-versions.js",
    "prepublishOnly": "vite build",
    "lint": "eslint --fix . --max-warnings=0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0 || ^10.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0 || ^10.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "22.16.0",
    "npm": "10.9.2"
  }
