{
  "name": "routerino-demo",
  "version": "1.0.0",
  "private": true,
  "description": "Demo website for Routerino - the lightweight React router",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t routerino-demo-prerender .",
    "docker:run": "docker run -p 8081:80 -p 3000:3000 --name routerino-demo-prerender routerino-demo-prerender",
    "docker:stop": "docker stop routerino-demo-prerender && docker rm routerino-demo-prerender",
    "prerender:up": "docker-compose up -d",
    "prerender:down": "docker-compose down",
    "prerender:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "routerino": "file:.."
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^4.13.7",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.5.8",
    "vite": "^6.3.5"
  }
}
